{
  "name": "n8n-nodes-gedcom",
  "version": "0.1.0",
  "description": "n8n community node for parsing GEDCOM genealogy files and computing ancestry trees",
  "keywords": [
    "n8n-community-node-package",
    "gedcom",
    "genealogy",
    "family-tree",
    "ancestry"
  ],
  "license": "MIT",
  "homepage": "https://github.com/salnikay/n8n-nodes-gedcom",
  "author": {
    "name": "n8n Community",
    "email": "community@n8n.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/salnika/n8n-nodes-gedcom.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons && node build.mjs",
    "dev": "tsc --watch",
    "format": "oxlint --fix",
    "lint": "oxlint",
    "lintfix": "oxlint --fix",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/Gedcom/Gedcom.node.bundled.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "esbuild": "^0.25.9",
    "esbuild-node-externals": "^1.18.0",
    "gulp": "^5.0.0",
    "n8n-workflow": "^1.82.0",
    "oxlint": "^1.12.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "parse-gedcom": "^2.0.1",
    "read-gedcom": "^0.3.2"
  }
}